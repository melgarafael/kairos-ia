# IA Console V2 (deprecated)
IA_CONSOLE_V2_RATE_LIMIT=5
FEATURE_IA_CONSOLE_V2=false
NEXT_PUBLIC_FEATURE_IA_CONSOLE_V2=false

# IA Console V3 - requer SUPABASE_SERVICE_ROLE_KEY, OPENAI_API_KEY e ADMIN_ANALYTICS_SECRET
FEATURE_IA_CONSOLE_V3=true
NEXT_PUBLIC_FEATURE_IA_CONSOLE_V3=true

# IA Console Vendas - Integração Ticto + Hotmart
# ==================================================

# TICTO - Gateway de Vendas
# Obtenha as credenciais no dashboard da Ticto: https://dashboard.ticto.io
TICTO_CLIENT_ID=your_ticto_client_id
TICTO_CLIENT_SECRET=your_ticto_client_secret
TICTO_API_BASE_URL=https://glados.ticto.cloud

# HOTMART - Plataforma de Produtos Digitais
# Obtenha as credenciais em: https://developers.hotmart.com
# Você pode usar Client ID/Secret OU Basic Auth (apenas um é necessário)
HOTMART_CLIENT_ID=your_hotmart_client_id
HOTMART_CLIENT_SECRET=your_hotmart_client_secret
# HOTMART_BASIC_AUTH=YmU4NDQ4YzctYzdhYS00YjAzLTliNjItNWUyM2M2ZWQyZDE3OmExYTRhNmY1LTk5MjctNDljYi1hZTBlLWU2ZTk4YmYxZWNkNA==
HOTMART_API_BASE_URL=https://developers.hotmart.com

# Feature flag para habilitar IA Console Vendas
FEATURE_IA_CONSOLE_VENDAS=true
NEXT_PUBLIC_FEATURE_IA_CONSOLE_VENDAS=true

# IA Console Memberkit (Acessos)
# ==================================================
# Obtenha sua API Key em: https://memberkit.com.br -> Configurações -> API
MEMBERKIT_API_KEY=your_memberkit_api_key
MEMBERKIT_API_BASE_URL=https://memberkit.com.br/api/v1

# Feature flag para habilitar IA Console Memberkit
FEATURE_IA_CONSOLE_MEMBERKIT=true
NEXT_PUBLIC_FEATURE_IA_CONSOLE_MEMBERKIT=true

# PostHog Analytics (opcional)
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=

